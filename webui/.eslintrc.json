{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "html"
    ],
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "nonblock-statement-body-position": ["error", "below"],
        "semi": "error"
    }
}
